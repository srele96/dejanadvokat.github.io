{
  "name": "static-lawyer-website",
  "description": "A static lawyer website.",
  "version": "0.0.1",
  "scripts": {
    "build:css": "tailwindcss -i ./input.css -o ./docs/output.css --minify --content './docs/**/*.{html,md}'",
    "build:css:watch": "tailwindcss -i ./input.css -o ./docs/output.css --watch --content './docs/**/*.{html,md}'"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.15",
    "tailwindcss": "^4.1.15"
  }
}
